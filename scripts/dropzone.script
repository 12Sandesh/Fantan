go.property("suit_code", hash(""))

function init(self)
    -- Get the suit code from the property
    local suit_hash = go.get("#", "suit_code")
    self.suit = suit_hash
    
    -- Get position for debugging
    local pos = go.get_position()
    print("Dropzone initialized - Suit:", suit_hash, "Position:", pos.x, pos.y)
end